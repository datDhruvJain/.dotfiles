"set guifont=Andale\ Mono:h13
set guifont=Monaco:h14

set guioptions=gm
" e for tabpages
" g for grey inactive window bars
" m for menu bar present
" no r for no right scrollbar
" no L for no Left scrollbar on split window

set laststatus=0
set cursorlineopt=number cursorline

set columns=122 lines=29
" change based on font, currently set for Monaco:h14

nmap <leader>t :tabnew<cr>
nmap <leader>n :19Lexplore!<cr>
